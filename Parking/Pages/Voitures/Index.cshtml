@page
@model Parking.Pages.Voitures.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Voiture[0].Matricule)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Voiture[0].Modele)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Voiture[0].Marque)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Voiture[0].Annee)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Voiture) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Matricule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modele)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marque)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Annee)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.VoitureId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.VoitureId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.VoitureId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
